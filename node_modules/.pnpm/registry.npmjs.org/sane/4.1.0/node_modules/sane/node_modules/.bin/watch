#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/vinnihoke/git-projects/projects/bvilding/time-blocking-tool-be/node_modules/.pnpm/registry.npmjs.org/@cnakazawa/watch/1.0.4/node_modules/@cnakazawa/watch/cli.js/node_modules:/Users/vinnihoke/git-projects/projects/bvilding/time-blocking-tool-be/node_modules/.pnpm/registry.npmjs.org/@cnakazawa/watch/1.0.4/node_modules/@cnakazawa/watch/node_modules:/Users/vinnihoke/git-projects/projects/bvilding/time-blocking-tool-be/node_modules/.pnpm/registry.npmjs.org/@cnakazawa/watch/1.0.4/node_modules/@cnakazawa/node_modules:/Users/vinnihoke/git-projects/projects/bvilding/time-blocking-tool-be/node_modules/.pnpm/registry.npmjs.org/@cnakazawa/watch/1.0.4/node_modules:/Users/vinnihoke/git-projects/projects/bvilding/time-blocking-tool-be/node_modules/.pnpm/registry.npmjs.org/@cnakazawa/watch/node_modules:/Users/vinnihoke/git-projects/projects/bvilding/time-blocking-tool-be/node_modules/.pnpm/registry.npmjs.org/@cnakazawa/node_modules:/Users/vinnihoke/git-projects/projects/bvilding/time-blocking-tool-be/node_modules/.pnpm/registry.npmjs.org/node_modules:/Users/vinnihoke/git-projects/projects/bvilding/time-blocking-tool-be/node_modules/.pnpm/node_modules:/Users/vinnihoke/git-projects/projects/bvilding/time-blocking-tool-be/node_modules:/Users/vinnihoke/git-projects/projects/bvilding/node_modules:/Users/vinnihoke/git-projects/projects/node_modules:/Users/vinnihoke/git-projects/node_modules:/Users/vinnihoke/node_modules:/Users/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@cnakazawa/watch/1.0.4/node_modules/@cnakazawa/watch/cli.js" "$@"
else 
  exec node  "$basedir/../../../../../../@cnakazawa/watch/1.0.4/node_modules/@cnakazawa/watch/cli.js" "$@"
fi
