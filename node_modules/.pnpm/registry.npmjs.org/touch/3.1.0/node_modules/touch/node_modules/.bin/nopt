#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/vinnihoke/git-projects/projects/bvilding/time-blocking-tool-be/node_modules/.pnpm/registry.npmjs.org/nopt/1.0.10/node_modules/nopt/bin/nopt.js/node_modules:/Users/vinnihoke/git-projects/projects/bvilding/time-blocking-tool-be/node_modules/.pnpm/registry.npmjs.org/nopt/1.0.10/node_modules/nopt/bin/node_modules:/Users/vinnihoke/git-projects/projects/bvilding/time-blocking-tool-be/node_modules/.pnpm/registry.npmjs.org/nopt/1.0.10/node_modules/nopt/node_modules:/Users/vinnihoke/git-projects/projects/bvilding/time-blocking-tool-be/node_modules/.pnpm/registry.npmjs.org/nopt/1.0.10/node_modules:/Users/vinnihoke/git-projects/projects/bvilding/time-blocking-tool-be/node_modules/.pnpm/registry.npmjs.org/nopt/node_modules:/Users/vinnihoke/git-projects/projects/bvilding/time-blocking-tool-be/node_modules/.pnpm/registry.npmjs.org/node_modules:/Users/vinnihoke/git-projects/projects/bvilding/time-blocking-tool-be/node_modules/.pnpm/node_modules:/Users/vinnihoke/git-projects/projects/bvilding/time-blocking-tool-be/node_modules:/Users/vinnihoke/git-projects/projects/bvilding/node_modules:/Users/vinnihoke/git-projects/projects/node_modules:/Users/vinnihoke/git-projects/node_modules:/Users/vinnihoke/node_modules:/Users/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../nopt/1.0.10/node_modules/nopt/bin/nopt.js" "$@"
else 
  exec node  "$basedir/../../../../../../nopt/1.0.10/node_modules/nopt/bin/nopt.js" "$@"
fi
