#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/vinnihoke/git-projects/projects/bvilding/time-blocking-tool-be/node_modules/.pnpm/registry.npmjs.org/esprima/4.0.1/node_modules/esprima/bin/esvalidate.js/node_modules:/Users/vinnihoke/git-projects/projects/bvilding/time-blocking-tool-be/node_modules/.pnpm/registry.npmjs.org/esprima/4.0.1/node_modules/esprima/bin/node_modules:/Users/vinnihoke/git-projects/projects/bvilding/time-blocking-tool-be/node_modules/.pnpm/registry.npmjs.org/esprima/4.0.1/node_modules/esprima/node_modules:/Users/vinnihoke/git-projects/projects/bvilding/time-blocking-tool-be/node_modules/.pnpm/registry.npmjs.org/esprima/4.0.1/node_modules:/Users/vinnihoke/git-projects/projects/bvilding/time-blocking-tool-be/node_modules/.pnpm/registry.npmjs.org/esprima/node_modules:/Users/vinnihoke/git-projects/projects/bvilding/time-blocking-tool-be/node_modules/.pnpm/registry.npmjs.org/node_modules:/Users/vinnihoke/git-projects/projects/bvilding/time-blocking-tool-be/node_modules/.pnpm/node_modules:/Users/vinnihoke/git-projects/projects/bvilding/time-blocking-tool-be/node_modules:/Users/vinnihoke/git-projects/projects/bvilding/node_modules:/Users/vinnihoke/git-projects/projects/node_modules:/Users/vinnihoke/git-projects/node_modules:/Users/vinnihoke/node_modules:/Users/node_modules:/node_modules:/Users/vinnihoke/git-projects/projects/bvilding/time-blocking-tool-be/node_modules/.pnpm/node_modules/esprima/bin/esvalidate.js/node_modules:/Users/vinnihoke/git-projects/projects/bvilding/time-blocking-tool-be/node_modules/.pnpm/node_modules/esprima/bin/node_modules:/Users/vinnihoke/git-projects/projects/bvilding/time-blocking-tool-be/node_modules/.pnpm/node_modules/esprima/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../esprima/bin/esvalidate.js" "$@"
else 
  exec node  "$basedir/../esprima/bin/esvalidate.js" "$@"
fi
